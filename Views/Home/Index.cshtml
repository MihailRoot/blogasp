@model IEnumerable<testaundit.Models.blog>;
@{
    ViewData["Title"] = "Blogs";
}
<div>
<h1>Anonymous blog</h1>
</div>
<h1>@ViewData["Title"]</h1>
    @foreach( var item in Model)
    {
        <p>@Html.DisplayNameFor(model => model.Name)
	    @Html.DisplayFor(modelItem => item.Name)</p>
       <p><br>@Html.DisplayNameFor(model => model.text)
       <br>@Html.DisplayFor(modelItem => item.text)</p>
    }